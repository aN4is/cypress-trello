    "target": "ES2020",
    "lib": ["ES2020", "dom"],
    "module": "ESNext",
    "moduleResolution": "node",
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["**/*.ts"]
}